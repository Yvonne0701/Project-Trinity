/* pages/wiki/wiki.wxss */
page{
  margin:0px;
  padding:0px;
  height:100%;
  width:100%;
  align-items: center;
  display: flex;
  flex-direction:column;
}
._box{
  position: fixed;
  width:350px;
  display: flex;
  flex-direction:row;
  z-index:1;
}
._tmp{
  position:fixed;
  width:100%;
  height:45px;
  top:0%;
  background-color: white;
}
._input{
  border:2px solid black;
  border-radius: 15px;
  padding:5px 15px;
  height:20px;
  width:250px;
  color:black;
}
._button{
  width:34px;
  height:34px;
}
._main{
  position:relative;
  width:80%;
  height:80%;
  top:50px;
}
.bg{
  position:fixed;
  top:0%;
  left:0%;
  height:120%;
  width:120%;
  z-index:-1;
}