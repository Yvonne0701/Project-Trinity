/* pages/test/test.wxss */
page{
  margin:0px;
  padding:0px;
  height:100%;
  width:100%;
  align-items: center;
  display: flex;
  flex-direction:column;
}
.h{
  height:50px;
  line-height: 50px;
  font-weight: 500;
}
.bg{
  position:fixed;
  top:0%;
  left:0%;
  height:120%;
  width:120%;
  z-index:-1;
}
._button{
  height: 100px;
  background: linear-gradient(to bottom, rgb(136, 211, 203) ,rgb(43, 202, 207)); 
  border-radius: 25px;
  border-bottom: 10px solid rgb(25, 170, 180);
  font-weight: 600;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.05);
  font-size: 20px;
  text-align: center;
  color:black;
  box-shadow: 0px 10px 0px rgba(0,0,0,.2);
  cursor: pointer;
}